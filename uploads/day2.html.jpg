<!DOCTYPE html>
<html>
<body>

<table id="table" border=1 width=100%>

</table>
<script>
	var table=document.getElementById("table") ;
	var s="";
	
	for(var i=0;i<9;i++)
	{
		s+="<tr>"
		for(var j=0;j<9;j++)
		{
			s+=`<td> ${i} , ${j}  </td>`
		
			
		}
		s+="</tr>"
	}
	

	
	table.innerHTML=s;
	console.log(s)
	
</script>
</body>

</html>


