<div class="attendance-summary">
    <div class="row">
        <div class="col_7">
            <div class="box">
                <label> Days</label>
                <p>
                    <%=data1.length  %>
                </p>
            </div>
        </div>

        <div class="col_7">
            <div class="box">
                <label> Late</label>
                <p>0% (0 Days)</p>
            </div>
        </div>

        <div class="col_7">
            <div class="box">
                <label> Absent</label>
                <p>0% (0 Days) </p>
            </div>
        </div>

        <div class="col_7">
            <div class="box">
                <label> Half Days</label>
                <p>0</p>
            </div>
        </div>



        <div class="col_7">
            <div class="box">
                <label> Total Office</label>
               <p><%=total_workhours%></p> 
                
            </div>
        </div>

        <div class="col_7">
            <div class="box">
                <label> Total worked</label>
                <p>
                    <%=all_work_hours %>
                </p>
            </div>
        </div>

        <div class="col_7">
            <div class="box">
                <label> Productivity Ratio</label>
            </div>
        </div>

    </div>


    <div class="table">
        <table>
            <tr style="background-color: #002f4b;">
                <th>Date </th>
                <th>Entry Time </th>
                <th>Exit time </th>
                <th>Break time </th>
                <th>Working Hours </th>


            </tr>
            <% for(i=0;i<data1.length;i++) {%>
                <tr>
                    <td>
                        <%= moment(data1[i].check_date).utcOffset(tz).format("YYYY-MM-DD") %>
                    </td>
                    <td>
                        <%=moment(data1[i].checkin_time).utcOffset(tz).format("hh:mm:ss")%>
                    </td>
                    <td>
                        <%=data1[i].checkout_time %>
                    </td>
                    <td>
                        <%= total_brake[i]  %>
                    </td>

                    <td><span><%=total_workhours[i] %> </span><progress id="file" value="32" max="100"></progress></td>


                    <%} %>




        </table>
    </div>
</div>
<script src="/js/moment.min.js"></script>